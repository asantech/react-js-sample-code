{
  "private": true,
  "workspaces": [
    "apps/react-19",
    "apps/react-18",
    "packages/ui"
  ],
  "scripts": {
    "start:react18": "npm run dev --workspace=apps/react-18",
    "start:react19": "npm run dev --workspace=apps/react-19",
    "build:react19": "npm run --workspace=react19-project build",
    "build:react18": "npm run --workspace=react18-project build",
    "build:all": "npm run build:react19 && npm run build:react18"
  }
}
